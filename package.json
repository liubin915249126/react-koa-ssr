{
    "name": "react-isomorphic-boilerplate",
    "version": "1.0.0",
    "description": "An universal React isomorphic boilerplate for building server-side render web app",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon ./server/server.dev.js --watch server --watch build",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js --progress",
        "clean": "rimraf ./dist/client/* && rimraf ./dist/server/* && rimraf ./views/prod/*",
        "lint": "eslint . --ext .js",
        "start": "npm run build && node ./dist/server/server.js",
        "pm2": "pm2"
    },
    "dependencies": {
        "ejs": "^2.5.5",
        "koa": "^2.0.0",
        "koa-bodyparser": "^3.2.0",
        "koa-compose": "^3.2.0",
        "koa-compress": "^2.0.0",
        "koa-convert": "^1.2.0",
        "koa-json": "^2.0.0",
        "koa-logger": "^2.0.0",
        "koa-router": "^7.0.0",
        "koa-session": "^3.4.0",
        "koa-static": "^3.0.0",
        "koa-views": "^5.0.0",
        "pm2": "^2.2.2",
        "react": "^15.3.1",
        "react-dom": "^15.3.1",
        "react-redux": "^5.0.1",
        "react-router": "^3.0.0",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "superagent": "^3.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.4.4",
        "asset-require-hook": "^1.0.2",
        "autoprefixer": "^6.4.1",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-hmre": "^1.1.1",
        "cross-env": "^3.1.3",
        "css-loader": "^2.1.1",
        "css-modules-require-hook": "^4.0.5",
        "eslint": "^3.12.2",
        "eslint-plugin-react": "^6.8.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "json-loader": "^0.5.7",
        "koa-webpack-dev-middleware": "^1.3.0",
        "koa-webpack-hot-middleware": "^1.0.3",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "^4.12.0",
        "nodemon": "^1.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "progress-bar-webpack-plugin": "^1.9.0",
        "redux-devtools": "^3.3.1",
        "redux-devtools-dock-monitor": "^1.1.1",
        "redux-devtools-log-monitor": "^1.1.1",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "source-map-support": "^0.4.10",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1"
    },
    "author": "Chikara Chan",
    "repository": {
        "type": "git",
        "url": "https://github.com/chikara-chan/react-isomorphic.git"
    },
    "license": "MIT"
}
